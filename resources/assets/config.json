{
  "entry": {
    "main-local": [
      "./scripts/main-local.js",
      "./styles/app-mobile.scss",
      "./styles/app-desktop.scss"
    ],
    "app-mobile": [
      "./scripts/app-mobile.js",
      "./styles/app-mobile.scss"
    ],
    "app-desktop": [
      "./scripts/app-desktop.js",
      "./styles/app-desktop.scss"
    ],
    "print": [
      "./styles/print.scss"
    ]
  },
  "publicPath": "/wp-content/themes/sage-theme",
  "devUrl": "http://localhost:3333",
  "proxyUrl": "http://localhost:3000",
  "cacheBusting": "[name]_[hash:8]",
  "watch": [
    "app/**/*.php",
    "config/**/*.php",
    "resources/views/**/*.php"
  ]
}
